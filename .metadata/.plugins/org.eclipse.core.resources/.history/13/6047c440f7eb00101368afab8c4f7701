//		System.out.println("Você digitou: " + k);
		
		// várias dados na mesma linha
		String x;
		int y;
		double z;		
		x = sc.next();
		y = sc.nextInt();
		z = sc.nextDouble();
		System.out.println("Você digitou: " + x + ", " + y + ", " + z);
		
		
		
		
		sc.close(); // encerrar o recurso		
