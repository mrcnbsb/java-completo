package application;

import java.util.Set;
import java.util.TreeSet;

public class Program {

	public static void main(String[] args) {

		//Set<T>
		// não admite repetições
		// elementos não possuem posição
		// interseção, união, diferença
		
//		Set<String> set = new HashSet<>(); //HashSet-> implementação mais rápida, mas não garante a ordem
		Set<String> set = new TreeSet<>(); //HashSet-> implementação mais rápida, mas não garante a ordem
		set.add("TV");
		set.add("Tablet");
		set.add("Notebook");
		
		
		System.out.println(set.contains("Notebook"));
		
		for(String p : set) {
			System.out.println(p);
		}

	}
}
