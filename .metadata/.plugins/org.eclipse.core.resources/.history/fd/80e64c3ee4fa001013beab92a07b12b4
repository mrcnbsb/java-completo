package application;

import java.util.Scanner;

import services.PrintService;
import services.PrintServiceObject;
import services.PrintServiceString;

public class Program {

	public static void main(String[] args) {
		//Generics -> uso comum em coleções
		
		Scanner sc = new Scanner(System.in);
		
		PrintService ps = new PrintService();
		PrintServiceString pss = new PrintServiceString();
		PrintServiceObject pso = new PrintServiceObject();
		
		System.out.print("How many values? ");
		int n = sc.nextInt();		
		
		pso.addValue("Maria");
		
		for (int i=0; i<n; i++) {
			Integer value = sc.nextInt();
			pso.addValue(value);
		}
		
		pso.print();
		Integer x = (Integer) pso.first(); //casting
		System.out.println("First: " + x);
		
		sc.close();
	}
}
