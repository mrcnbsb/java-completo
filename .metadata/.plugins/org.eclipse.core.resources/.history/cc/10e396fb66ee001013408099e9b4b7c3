		//Exercício 2
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		while(x == 0 || y == 0) {
			if (x>0 && y>0) {
				System.out.println("primeiro");
			} else if (x>0 && y<0) {
				System.out.println("quarto");
			} else if (x<0 && y>0) {
				System.out.println("terceiro");
			} else {
				System.out.println("segundo");
			}
		}
		
		sc.close();
		
		
		
	}
}
