import java.util.Locale;
import java.util.Scanner;

public class Matrizes {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		//instanciação da matriz na memória
		int[][] mat = new int[n][n];
		
		sc.close();

	}

}
