		//Exercício 4
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int a, b;
		double result;
		for (int i=0; i<n; i++) {
			a = sc.nextInt();
			b = sc.nextInt();
			if (b == 0) {
				System.out.println("divisao impossivel");
			} else {
				result = (double) a / b;
				System.out.printf("%.1f%n", result);
			}
		}				
		sc.close();		
		
		
		
		
