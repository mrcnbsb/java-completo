import java.util.Scanner;

public class Exercicios_condicionais {
	public static void main(String[] args) {
		//ExercÃ­cio 1
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		if (num >= 0) {
			System.out.println("NAO NEGATIVO");
		}
		else {
			System.out.println("NEGATIVO");
		}
		
	}
}
