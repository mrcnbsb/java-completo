import java.util.Locale;
import java.util.Scanner;

public class Exercicios_matriz {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int m = sc.nextInt();
		int n = sc.nextInt();
		
		int[][] mat = new int[m][n];
		
		for(int i=0; i<m; i++) {
			for(int j=0; j<n; j++) {
				mat[i][j] = sc.nextInt();
			}
		}
		
		int num = sc.nextInt();
		for(int i=0; i<m; i++) {
			for(int j=0; j<n; j++) {
				if(mat[m][n] == num) {
					System.out.printf("Position %d,%d%n", m, n);
				}
			}
		}	
		
		
		
		sc.close();
	}
}
